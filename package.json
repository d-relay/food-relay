{
  "name": "food-relay",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "tsc": "tsc",
    "dev": "cross-env NODE_ENV=development node ./dist/bin/www",
    "build": "tsc",
    "postinstall": "npm run tsc",
    "start": "cross-env NODE_ENV=production node ./dist/bin/www",
    "db:migrate": "ts-node $(npm bin)/typeorm migration:run",
    "db:generate": "ts-node $(npm bin)/typeorm migration:generate",
    "db:create": "ts-node $(npm bin)/typeorm migration:create"
  },
  "dependencies": {
    "@koa/router": "^9.0.1",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "koa": "^2.12.0",
    "koa-body": "^4.1.3",
    "koa-hbs": "^1.0.0",
    "koa-morgan": "^1.0.1",
    "koa-passport": "^4.1.3",
    "koa-session": "^6.0.0",
    "node-fetch": "^2.6.0",
    "passport-twitch": "d-relay/passport-twitch",
    "pg": "^8.2.1",
    "rotating-file-stream": "^2.1.1",
    "typeorm": "^0.2.25",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.3"
  },
  "devDependencies": {
    "@koa/cors": "^3.1.0",
    "@types/debug": "^4.1.5",
    "@types/koa": "^2.11.3",
    "@types/koa-ejs": "^4.2.2",
    "@types/koa-hbs": "^1.0.5",
    "@types/koa-morgan": "^1.0.4",
    "@types/koa-passport": "^4.0.2",
    "@types/koa-session": "^5.10.2",
    "@types/koa__cors": "^3.0.1",
    "@types/koa__router": "^8.0.2",
    "@types/node": "^14.0.10",
    "@types/node-fetch": "^2.5.7"
  },
  "engines": {
    "node": ">=14"
  }
}